    "eleven-agent-mcp": {
      "command": "node",
      "args": ["/Users/nexteleven/cursor-mcp-integration/server/local-mcp-server.js"],
      "cwd": "/Users/nexteleven/cursor-mcp-integration",
      "env": {
        "VERCEL_APP_URL": "https://grokcode.vercel.app/",
