  "customTools": {
    "deploy": {
      "path": "./tools/deploy-mcp.js",
      "description": "Deploy to Vercel, Railway, AWS, GCP, Azure",
      "args": ["platform", "project", "token", "branch"],
      "platforms": ["vercel", "railway", "aws", "gcp", "azure"]
    },
    "tester": {
      "path": "./tools/tester-mcp.js",
      "description": "Run tests via MCP",
      "args": ["cmd", "type"],
      "types": ["unit", "e2e", "integration", "all"]
    },
    "ml": {
      "path": "./tools/ml-mcp.js",
      "description": "AI/ML training and deployment (Ollama, Jupyter, W&B)",
      "args": ["model", "data", "action", "backend"],
      "backends": ["ollama", "jupyter", "wandb"],
      "actions": ["train", "run", "deploy", "eval"]
    },
    "mobile": {
      "path": "./tools/mobile-mcp.js",
      "description": "Mobile app testing (Flutter, Swift, iOS, Android)",
      "args": ["platform", "action", "app"],
      "platforms": ["flutter", "swift", "ios", "android"],
      "actions": ["test", "build", "run", "doctor"]
    },
    "security": {
      "path": "./tools/security-mcp.js",
      "description": "Security scanning (npm audit, Snyk, SonarQube)",
      "args": ["scanner", "action"],
      "scanners": ["npm", "snyk", "sonarqube", "all"],
      "actions": ["scan", "fix", "report"]
    },
    "infra": {
      "path": "./tools/infra-mcp.js",
      "description": "Infrastructure-as-code (Terraform, ArgoCD, AWS/GCP/Azure)",
      "args": ["action", "provider", "target", "env"],
      "providers": ["terraform", "argocd", "aws", "gcp", "azure"],
      "actions": ["plan", "apply", "destroy", "sync", "status", "deploy"],
      "environments": ["dev", "staging", "prod"]
    },
    "qa": {
      "path": "./tools/qa-mcp.js",
      "description": "QA automation (Playwright, Cypress, Vitest, Load Testing)",
      "args": ["framework", "action", "target"],
      "frameworks": ["playwright", "cypress", "vitest", "load"],
      "actions": ["test", "ui", "headed", "debug", "report"]
    },
    "swarm": {
      "description": "Agent swarm orchestration via MCP[query_agent]",
      "usage": "/swarm [agent-type] [task]",
      "agents": ["devops", "ml", "mobile", "security", "a11y", "fullstack", "qa", "infra", "observability", "performance", "compliance"]
    }
  },
  "notes": "CTO Max Pack - 200+ abilities via MCP/Eleven. All tools accessible via repo tools/"
}
